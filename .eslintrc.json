{
    "parser": "babel-eslint",
    "extends": ["airbnb", "prettier"],
    "plugins": [
        "prettier",
        "eslint-plugin-import"
    ],
    "rules": {
        "import/prefer-default-export": 0,
        "import/no-useless-path-segments": 0,
        "prettier/prettier": 2,
        "react/no-unescaped-entities": 0,
        "react/jsx-one-expression-per-line": 0
    }
}